{
  "name": "kodguru-swrepo",
  "version": "2.1.1",
  "author": "jwennerberg",
  "summary": "Manage software repositories",
  "license": "Apache-2.0",
  "source": "https://github.com/kodguru/puppet-module-swrepo.git",
  "project_page": "https://github.com/kodguru/puppet-module-swrepo",
  "issues_url": "https://github.com/kodguru/puppet-module-swrepo/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "SLED",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.10.0 < 6.0.0"},
    {"name":"voxpupuli/zypprepo","version_requirement":">=1.0.0 < 3.0.0"},
    {"name":"stschulte/rpmkey","version_requirement":">=1.0.0 < 2.0.0"},
    {"name":"puppetlabs/yumrepo","version_requirement":">= 1.0.0 < 2.0.0"}
  ]
}
